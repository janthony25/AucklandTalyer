@model tblParts

<div class="container">
    <h2>Add Parts</h2>
    <form asp-action="Create" method="post">
        <div class="form-group">
            <label asp-for="PartsName"></label>
            <input asp-for="PartsName" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="PartsPrice"></label>
            <input asp-for="PartsPrice" class="form-control" />
        </div>
        <button type="submit" style="width=150px">Add</button>
    </form>

</div>